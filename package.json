    "@angular/service-worker": "15.2.9",
    "@ibm/type": "^0.5.4",
    "core-js": "^3.31.0",
    "firebase": "^9.22.2",
    "jasmine": "^4.6.0",
    "ngx-markdown": "^14.0.1",
    "prismjs": "^1.29.0",
    "rxfire": "^6.0.3",
    "rxjs": "^7.8.1",
    "tslib": "^2.5.3",
    "zone.js": "~0.12.0"
  },
  "devDependencies": {
    "@angular-devkit/architect": "^0.1402.11",
    "@angular-devkit/build-angular": "~14.2.11",
