  "license": "MIT",
  "author": "Eric N. Garcia",
  "description": "This is a progressive web app (PWA) that is written in Angular.  It serves as my personal portfolio",
  "repository": {
    "type": "git",
    "url": "https://github.com/ENG618/pwa.garciaericn"
  },
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build --configuration production",
    "postinstall": "ngcc",
    "test": "ng test",
    "lint": "ng lint",
    "stylelint": "stylelint '**/*.scss'",
    "stylelint:fix": "stylelint '**/*.scss' --fix",
    "format": "prettier --write \"src/**/*.{scss,css,js,md,mdx,ts}\"",
    "format:diff": "prettier --check \"src/**/*.{scss,css,js,md,mdx,ts}\"",
    "e2e": "ng e2e",
    "deploy": "ng build --prod && firebase deploy",
    "compodoc": "npx compodoc -p src/tsconfig.app.json -d docs -s"
  },
  "private": true,
