{
  "name": "garciaericn",
  "version": "0.0.0",
  "license": "MIT",
  "author": "Eric N. Garcia",
  "description": "This is a progressive web app (PWA) that is written in Angular.  It serves as my personal portfolio",
  "repository": {
    "type": "git",
    "url": "https://github.com/ENG618/pwa.garciaericn"
  },
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "build:prod": "ng build --configuration production",
    "postinstall": "ngcc",
    "test": "ng test --watch",
    "test:ci": "ng test --browsers=ChromeHeadless --watch=false",
    "lint": "ng lint",
    "stylelint": "stylelint '**/*.scss'",
    "stylelint:fix": "stylelint '**/*.scss' --fix",
    "format": "prettier --check \"src/**/*.{scss,css,js,md,mdx,ts}\"",
    "format:fix": "prettier --write \"src/**/*.{scss,css,js,md,mdx,ts}\"",
    "e2e": "ng e2e",
    "deploy": "ng build --configuration production && firebase deploy",
    "compodoc": "npx compodoc -p src/tsconfig.app.json -d docs -s"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "13.2.3",
